        <script type="text/javascript">
          region.isAdmin = false;
          var load_cities = 0;
          var load_districts = 0;
          {literal}
            onload = function() {
              Ajax.call(region.getFileName(), 'type=1' + '&target=' + 'search_selProvinces' + '&parent=1' + '&value={$search_province}', region.response, "GET", "JSON");
              Ajax.call(region.getFileName(), 'type=2' + '&target=' + 'search_selCities' + '&parent={$search_province}' + '&value={$search_city}', region.response, "GET", "JSON");
              Ajax.call(region.getFileName(), 'type=3' + '&target=' + 'search_selDistricts' + '&parent={$search_city}' + '&value={$search_district}', region.response, "GET", "JSON");
            }
            function reload_cities(obj) {
              {if $search_city > 0}
                if ( load_cities > 0 ) {
                    region.changed(obj, 2, 'search_selCities');
                }
              {else}
                region.changed(obj, 2, 'search_selCities');
              {/if}
              load_cities = load_cities + 1;
            }
            function reload_districts(obj) {
              {if $search_district > 0}
                if ( load_districts > 0 ) {
                    region.changed(obj, 3, 'search_selDistricts');
                }
              {else}
                region.changed(obj, 3, 'search_selDistricts');
              {/if}
              load_districts = load_districts + 1;
            }
          {/literal}
