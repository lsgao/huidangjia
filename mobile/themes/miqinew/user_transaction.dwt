                  region.changed(obj, 2, 'search_selCities');
              }
            {else}
              region.changed(obj, 2, 'search_selCities');
            {/if}
            load_cities = load_cities + 1;
          }
          function reload_districts(obj) {
            {if $search_district > 0}
              if ( load_districts > 0 ) {
                  region.changed(obj, 3, 'search_selDistricts');
              }
            {else}
              region.changed(obj, 3, 'search_selDistricts');
            {/if}
            load_districts = load_districts + 1;
          }
        {/literal}
